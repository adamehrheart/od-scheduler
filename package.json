{
  "name": "od-scheduler",
  "version": "1.0.0",
  "description": "Open Dealer Scheduler Service - Manages data pull scheduling and monitoring",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm run build && doppler run -- node dist/src/dev-server.js",
    "dev:vercel": "vercel dev",
    "build": "tsc",
    "deploy": "vercel --prod",
    "test": "vitest run",
    "test:scheduler": "doppler run -- node scripts/test-scheduler.mjs",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "queue:status": "doppler run -- node scripts/check-job-queue.mjs",
    "queue:health": "doppler run -- node scripts/job-queue-health-check.mjs",
    "queue:health:detailed": "doppler run -- node scripts/job-queue-health-check.mjs --detailed",
    "queue:manage": "doppler run -- node scripts/manage-job-queue.mjs",
    "queue:reset-stuck": "doppler run -- node scripts/manage-job-queue.mjs reset-stuck",
    "queue:retry-failed": "doppler run -- node scripts/manage-job-queue.mjs retry-failed",
    "queue:cleanup": "doppler run -- node scripts/manage-job-queue.mjs cleanup-old",
    "queue:analyze-errors": "doppler run -- node scripts/analyze-job-errors.mjs",
    "queue:test-enhanced": "doppler run -- node scripts/test-enhanced-jobs.mjs"
  },
  "dependencies": {
    "@adamehrheart/schema": "file:../od-schema",
    "@supabase/supabase-js": "^2.55.0",
    "@types/next": "^8.0.7",
    "cheerio": "^1.1.2",
    "dotenv": "^16.4.7",
    "fast-xml-parser": "^5.2.5",
    "node-cron": "^3.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.17.2",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/node": "^3.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}